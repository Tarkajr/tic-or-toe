<h1>Sign up:</h1>
<div class="forms">
  <%= form_for :user, url: '/users' do |f| %>
    <div class="row">
      <form class="col s12">
        <div class="row">
          <div class="input-field col s6">
            Username:
            <%= f.text_field :username %>
            <label for="username"></label>


    <div class="row">
      <div class="input-field col s12">
          Password:
          <%= f.password_field :password %>
            <label for="password"></label>

        <button class="btn waves-effect waves-light" type="submit" name="action"<%= f.submit "Register"%>Register
  </button>
    <%end %>


      </form>
    </div>
    </div>
